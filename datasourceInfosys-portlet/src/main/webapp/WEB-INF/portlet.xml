<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>MockData</portlet-name>
		<display-name>MockData</display-name>
		<portlet-class>javax.portlet.faces.GenericFacesPortlet</portlet-class>
		<init-param>
			<name>javax.portlet.faces.defaultViewId.view</name>
			<value>/views/mockData/mockDataPortletViewMode.xhtml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>MockData</title>
			<short-title>MockData</short-title>
			<keywords>MockData</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>ReceivedFeedback</portlet-name>
		<display-name>ReceivedFeedback</display-name>
		<portlet-class>javax.portlet.faces.GenericFacesPortlet</portlet-class>
		<init-param>
			<name>javax.portlet.faces.bridgeEventHandler</name>
			<value>de.hska.wi.awp.datasource.infosys.event.ReceivedFeedbackEventHandler</value>
		</init-param>
		<init-param>
			<name>javax.portlet.faces.defaultViewId.view</name>
			<value>/views/receivedFeedback/receivedFeedback.xhtml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>ReceivedFeedback</title>
			<short-title>ReceivedFeedback</short-title>
			<keywords>ReceivedFeedback</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-processing-event>
    		<qname xmlns:x="http://liferay.com/events">x:ipc.studentSelected</qname>
		</supported-processing-event>
		<supported-processing-event>
    		<qname xmlns:x="http://liferay.com/events">x:ipc.projectSelected</qname>
		</supported-processing-event>
	</portlet>
	<portlet>
		<portlet-name>Navigation</portlet-name>
		<display-name>Navigation</display-name>
		<portlet-class>javax.portlet.faces.GenericFacesPortlet</portlet-class>
		<init-param>
			<name>javax.portlet.faces.defaultViewId.view</name>
			<value>/views/navigation/navigation.xhtml</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Navigation</title>
			<short-title>Navigation</short-title>
			<keywords>Navigation</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-publishing-event>
    		<qname xmlns:x="http://liferay.com/events">x:ipc.studentSelected</qname>
		</supported-publishing-event>
		<supported-publishing-event>
    		<qname xmlns:x="http://liferay.com/events">x:ipc.projectSelected</qname>
		</supported-publishing-event>
	</portlet>
	<event-definition>
    	<qname xmlns:x="http://liferay.com/events">x:ipc.studentSelected</qname>
    	<value-type>java.lang.String</value-type>
    </event-definition>
    <event-definition>
    	<qname xmlns:x="http://liferay.com/events">x:ipc.projectSelected</qname>
    	<value-type>java.lang.String</value-type>
    </event-definition>
</portlet-app>
